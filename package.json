{
  "name": "karcin-ui",
  "version": "1.0.31",
  "description": "Kar√ßin UI - React & Typescript",
  "repository": "https://github.com/ysimsek/karcin-ui",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --color --port 3000",
    "build": "webpack --progress --profile --colors -p && Echo Showcase Build completed!",
    "lib": "cd src && tsc && mkdir -p ../lib/css && cp -R ./css/sass ../lib/css/sass && echo f | cp -R ./css/karcin-ui.css ../lib/css/karcin-ui.css && cp -R ../package.json ../lib/package.json && Echo Library Build completed!",
    "publish:lib": "cd lib && npm publish && Echo Library Npm Publish completed!"
  },
  "keywords": [
    "react",
    "typescript",
    "webpack"
  ],
  "author": "asis elektronik",
  "license": "ISC",
